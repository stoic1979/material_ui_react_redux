{
  "presets": [
    "es2015",
    "react",
    "stage-1"
  ],
  "env": {
    "development": {
      "presets": [
        "react-hmre"
      ]
    },
    "production": {
      "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "style": true
        }
      ],
      "transform-class-properties",
      "transform-react-constant-elements",
      "transform-react-remove-prop-types"
    ]
    }
    
  }
}
